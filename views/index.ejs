    width: 25%; /* Forces 4 columns */
    box-shadow: 0 4px 6px rgba(0,0,0,0.05);
    transition: transform 0.2s ease, border-color 0.2s ease;
}

/* Hover Effect on Cards */
.project-gallery td:hover {
    transform: translateY(-5px); /* Lifts up */
    border-color: #a18cd1; /* Turns purple */
    box-shadow: 0 10px 20px rgba(0,0,0,0.1);
}

/* Images inside cards */
.project-gallery img {
    max-width: 100%;
    height: auto;
    border-radius: 10px;
    margin-bottom: 12px;
    display: block;
    margin-left: auto;
    margin-right: auto;
}

/* Typography inside cards */
.project-gallery td {
    font-family: 'Nunito', sans-serif;
    font-size: 15px;
    font-weight: 700;
    color: #444;
}

/* --- SECTION HEADERS (First Summer Sewer, etc) --- */
/* Styling the rows that act as titles */
.section-header {
    background-color: #a18cd1 !important; /* Purple background */
    color: white !important;
    font-family: 'Fredoka', sans-serif;
    font-size: 26px;
    padding: 15px 0;
    border: none !important;
    border-radius: 10px;
    text-align: center;
    box-shadow: 0 4px 8px rgba(161, 140, 209, 0.4);
    /* Override grid styles for headers */
    width: auto !important; 
    border-spacing: 0;
}

/* Remove hover effect from headers */
.project-gallery td.section-header:hover {
    transform: none;
    cursor: default;
}

/* Clean up empty cells */
.project-gallery td:empty {
    border: none;
    background: transparent;
    box-shadow: none;
}
</style>

